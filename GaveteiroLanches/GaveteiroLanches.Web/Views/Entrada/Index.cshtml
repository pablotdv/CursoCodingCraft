@model IEnumerable<GaveteiroLanches.Web.ViewModels.Entrada.IndexViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Nova entrada", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DataHora)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fornecedor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MovimentacaoId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DataHora)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fornecedor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MovimentacaoId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new {  id=item.MovimentacaoId }) |
            @Html.ActionLink("Details", "Details", new {  id=item.MovimentacaoId }) |
            @Html.ActionLink("Delete", "Delete", new {  id=item.MovimentacaoId })
        </td>
    </tr>
}

</table>
